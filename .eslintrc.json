{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "quotes": ["error", "double"],
    "no-underscore-dangle": "off",
    "no-param-reassign": ["error"]
  },
  "env": {
    "node": true
  },
  "globals": {
    "after": true,
    "assert": true,
    "before": true,
    "describe": true,
    "it": true
  }
}
